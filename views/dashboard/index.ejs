<div class="dashboard-container" ng-controller="dashboardCtr">

</div>
<script type="text/javascript">
$(document).ready(function () {
    $.ajax({
        type:"get",
        url:"/project/config?name=<%-project%>",
        async:false,
        success:function (config) {
            var $container = $(".dashboard-container");
            $container.html(config.html);
            window.dashboardConfig = config.setting;
            angular.bootstrap($container[0], ["dashboardApp"])
        }
    });
});
</script>